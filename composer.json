{
    "name": "skorkito/nbp-php",
    "description": "API for accessing NBP (Narodowy Bank Polski) currency rates in PHP",
    "keywords": ["nbp", "currency", "money"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Sasinowski",
            "email": "tomasz@edualy.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "skorkito\\NbpPhp\\": "src/",
            "skorkito\\NbpPhp\\": "tests/php-unit/src/",
            "skorkito\\NbpPhp\\": "tests/behat/src/"
        }
    },
    "require-dev": {
        "php": ">=7.2",
        "behat/behat": "^3.4",
        "behat/mink": "^1.7",
        "doctrine/cache": "~1.6.2",
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^8.1"
    },
    "require": {
        "php": ">=7.0.0"
    },
    "suggest": {
        "doctrine/cache": "Allows local cache"
    }
}
